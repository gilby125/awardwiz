{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Marked Fares Worker",
      "request": "launch",
      "runtimeArgs": [
        "exec",
        "vite-node",
        "src/workers/marked-fares.ts"
      ],
      "runtimeExecutable": "pnpm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Scraper Remote",
      "runtimeExecutable": "pnpm",
      "runtimeArgs": [
        "exec",
        "vite-node",
        "src/scrapers/debug.ts",
        "--",
        "${input:params}"
      ],
      "skipFiles": ["<node_internals>/**", "**/node_modules/**"],
      "internalConsoleOptions": "openOnSessionStart",
    }
  ],
  "inputs": [
    {
      "id": "params",
      "type": "command",
      "command": "memento.promptString",
      "args": {
        "id": "params",
        "description": "Parameters? (from, to, date, scraper, chromium|browserless-func|browserless-websockets, ip)",
        "default": "YOW,YYZ,2022-10-20,aeroplan,browserless-func,10.0.1.1"
      }
    }
  ]
}
