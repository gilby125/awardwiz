{
  "name": "awardwiz",
  "private": true,
  "version": "2.0.0",
  "packageManager": "pnpm@7.14.0",
  "scripts": {
    "test": "vitest",
    "test-scrapers": "VITE_LIVE_SCRAPER_TESTS=1 vitest scrapers",
    "coverage": "vitest run --coverage",
    "build": "pnpm run schemas && tsc && vite build",
    "preview": "vite preview",
    "start": "pnpm run schemas && vite",
    "depcheck": "depcheck --ignores @iconify/json,~icons,@vitest/coverage-c8,vite-node",
    "schemas": "json2ts -i 'src/**/*.schema.json' -o 'src/types/'",
    "prepare": "husky install",
    "check": "pnpm run depcheck && echo 'linting' && TIMING=1 eslint --cache . && pnpm test -- run && SHOW_VIZ=false pnpm run build -- --log-level=error && echo 'ok'",
    "lets-upgrade-packages": "NPM_CHECK_INSTALLER=pnpm npm-check -u",
    "marked-fares-worker": "NODE_OPTIONS=--no-experimental-fetch vite-node src/workers/marked-fares.ts",
    "preview-email-worker": "vite-node src/workers/preview-email.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@react-oauth/google": "^0.2.8",
    "@supabase/supabase-js": "^2.0.4",
    "@tanstack/query-sync-storage-persister": "^4.13.0",
    "@tanstack/react-query": "^4.13.0",
    "@tanstack/react-query-devtools": "^4.13.0",
    "@tanstack/react-query-persist-client": "^4.13.0",
    "@textea/json-viewer": "^2.10.0",
    "antd": "^4.23.6",
    "axios": "^1.1.3",
    "dayjs": "^1.11.6",
    "rc-picker": "^2.6.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.8.4"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "7.6.6",
    "@iconify/json": "2.1.127",
    "@svgr/core": "^6.5.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/nodemailer": "^6.4.6",
    "@types/react": "18.0.23",
    "@types/react-dom": "18.0.7",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@typescript-eslint/eslint-plugin": "5.41.0",
    "@typescript-eslint/parser": "5.41.0",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-c8": "^0.24.3",
    "depcheck": "^1.4.3",
    "eslint": "^8.26.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^44.0.2",
    "handlebars": "^4.7.7",
    "husky": "^8.0.1",
    "jsdom": "^20.0.1",
    "json-schema-to-typescript": "^11.0.2",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "listr2": "^5.0.5",
    "node-fetch": "^3.2.10",
    "nodemailer": "^6.8.0",
    "npm-check": "^6.0.1",
    "p-wait-for": "^5.0.0",
    "puppeteer": "19.1.2",
    "rollup-plugin-visualizer": "^5.8.3",
    "unplugin-icons": "^0.14.12",
    "vite": "^3.1.8",
    "vite-node": "^0.24.3",
    "vite-plugin-imp": "^2.3.0",
    "vite-plugin-watch-and-run": "^1.0.3",
    "vitest": "^0.24.3"
  }
}
