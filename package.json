{
  "name": "awardwiz",
  "private": true,
  "version": "2.0.0",
  "scripts": {
    "test": "vitest",
    "test-scrapers": "VITE_LIVE_SCRAPER_TESTS=1 vitest scrapers",
    "coverage": "vitest run --coverage",
    "build": "npm run schemas && tsc && vite build",
    "preview": "vite preview",
    "start": "npm run schemas && vite",
    "depcheck": "npx depcheck --ignores @iconify/json,~icons,@vitest/coverage-c8,vite-node",
    "schemas": "npx json2ts -i 'src/**/*.schema.json' -o 'src/types/'",
    "prepare": "npm exec -- husky install",
    "check": "npm run depcheck && echo 'linting' && npx eslint --cache . && npm test -- run && SHOW_VIZ=false npm run build -- --log-level=error && echo 'ok'",
    "lets-upgrade-packages": "npx npm-check -u",
    "run-marked-fares-worker": "NODE_OPTIONS=--no-experimental-fetch npx vite-node src/utils/run-marked-fares-worker.tsx"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@react-oauth/google": "^0.2.8",
    "@supabase/supabase-js": "^2.0.0-rc.4",
    "@tanstack/query-sync-storage-persister": "^4.3.9",
    "@tanstack/react-query": "^4.3.9",
    "@tanstack/react-query-devtools": "^4.3.9",
    "@tanstack/react-query-persist-client": "^4.3.9",
    "@textea/json-viewer": "^2.7.1",
    "antd": "^4.23.2",
    "axios": "^0.27.2",
    "dayjs": "^1.11.5",
    "rc-picker": "^2.6.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^4.8.3"
  },
  "devDependencies": {
    "@iconify/json": "^2.1.111",
    "@kitql/vite-plugin-watch-and-run": "^0.4.2",
    "@svgr/core": "^6.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-react": "^2.1.0",
    "@vitest/coverage-c8": "^0.23.4",
    "depcheck": "^1.4.3",
    "eslint": "^8.23.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jsdom": "^20.0.0",
    "json-schema-to-typescript": "^11.0.2",
    "less": "^4.1.3",
    "lint-staged": "^13.0.3",
    "puppeteer": "^18.0.5",
    "rollup-plugin-visualizer": "^5.8.1",
    "unplugin-icons": "^0.14.10",
    "vite": "^3.1.3",
    "vite-node": "^0.23.4",
    "vite-plugin-imp": "^2.3.0",
    "vitest": "^0.23.4"
  }
}
